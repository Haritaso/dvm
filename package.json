{
  "name": "dvm",
  "version": "0.1.0",
  "description": "Fast and simple Deno version manager, built-in Native Reason",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "private": true,
  "esy": {
    "build": "refmterr dune build",
    "release": {
      "releasedBinaries": [
        "dvm.exe"
      ]
    }
  },
  "buildDirs": {
    "executable": {
      "name": "dvm.exe",
      "main": "Index",
      "require": [
        "base",
        "cmdliner",
        "console.lib",
        "dvm.lib",
        "lwt",
        "pastel.lib"
      ]
    },
    "library": {
      "name": "dvm.lib",
      "namespace": "Dvm",
      "require": [
        "base",
        "cohttp-lwt-unix",
        "cohttp-lwt",
        "cohttp",
        "console.lib",
        "lwt",
        "pastel.lib"
      ]
    },
    "test": {
      "name": "test.exe",
      "main": "Test",
      "require": [
        "dvm.lib",
        "rely.lib"
      ],
      "flags": [
        "-g",
        "-linkall"
      ]
    }
  },
  "scripts": {
    "pesy": "env PESY_MODE=update pesy",
    "fmt": "bash -c 'esy refmt --parse=re --in-place **/*.re'"
  },
  "dependencies": {
    "@opam/base": "v0.12.2",
    "@opam/cmdliner": "1.0.4",
    "@opam/cohttp": "2.4.0",
    "@opam/cohttp-lwt": "2.4.0",
    "@opam/cohttp-lwt-unix": "2.4.0",
    "@opam/lwt": "4.4.0",
    "@opam/lwt_ppx": "1.2.4",
    "@opam/lwt_ssl": "1.1.3",
    "@reason-native/console": "0.1.0",
    "@reason-native/pastel": "0.2.2",
    "@reason-native/rely": "3.1.0",
    "@esy-ocaml/reason": "3.5.2"
  },
  "devDependencies": {
    "@opam/dune": "1.11.4",
    "@opam/merlin": "3.3.2",
    "ocaml": "4.8.1000",
    "pesy": "0.4.4",
    "refmterr": "3.2.2"
  }
}

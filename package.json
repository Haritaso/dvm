  },
  "dependencies": {
    "@opam/cmdliner": "1.0.4",
    "@opam/core": "v0.12.4",
    "@opam/core_kernel": "v0.12.3",
    "@opam/dune": "1.11.4",
    "@opam/ezgzip": "0.2.3",
    "@opam/ocurl": "0.9.0",
    "@opam/rresult": "0.6.0",
    "@opam/yojson": "1.7.0",
    "ocaml": "4.09.0"
  },
  "devDependencies": {
    "@opam/merlin": "3.3.3",
    "@opam/ocamlformat": "0.12"
  }
}

{
  "name": "dvm",
  "version": "1.0.0",
  "description": "Fast and simple Deno runtime version manager built with Native Reason",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "esy": {
    "build": "dune build --root . --only-packages #{self.name} --ignore-promoted-rules --no-config"
  },
  "scripts": {
    "fmt": "bash -c 'esy refmt --parse=re --in-place **/*.re'"
  },
  "dependencies": {
    "@esy-ocaml/reason": "3.5.2",
    "@opam/cmdliner": "1.0.4",
    "@opam/core": "v0.12.4",
    "@opam/core_kernel": "v0.12.3",
    "@opam/dune": "1.11.4",
    "@opam/ezgzip": "0.2.3",
    "@opam/ocurl": "0.9.0",
    "@opam/rresult": "0.6.0",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "0.1.0",
    "@reason-native/pastel": "0.2.2",
    "ocaml": "4.09.0"
  },
  "devDependencies": {
    "@opam/merlin": "3.3.3"
  }
}

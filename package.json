{
  "name": "dvm",
  "version": "1.0.0",
  "description": "Fast and simple Deno runtime version manager written in OCaml",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "esy": {
    "build": "dune build -p #{self.name}"
  },
  "scripts": {

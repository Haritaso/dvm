{
  "name": "dvm",
  "version": "0.1.0",
  "description": "Fast and simple Deno version manager, built-in Native Reason",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "private": true,
  "esy": {
    "build": "refmterr dune build",
    "release": {
      "releasedBinaries": [
        "dvm.exe"
      ]
    }
  },
  "buildDirs": {
    "executable": {
      "name": "dvm.exe",
      "main": "Index",
      "modes": [
        "native",
        "exe"
      ],
      "require": [
        "cmdliner",
        "console.lib",
        "dvm.lib",
        "pastel.lib"
      ]
    },
    "library": {
      "name": "dvm.lib",
      "namespace": "Dvm",
      "preprocess": [
        "pps",
        "ppx_deriving_yojson",
        "ppx_deriving.eq",
        "ppx_deriving.make",
        "ppx_deriving.ord",
        "ppx_deriving.show",
        "ppx_let"
      ],
      "require": [
        "base",
        "console.lib",
        "core_kernel",
        "core",
        "curl",
        "pastel.lib",
        "ppx_deriving_yojson.runtime"
      ]
    },
    "test": {
      "name": "test.exe",
      "main": "Test",
      "modes": [
        "native",
        "exe"
      ],
      "require": [
        "dvm.lib",
        "rely.lib"
      ],
      "flags": [
        "-g",
        "-linkall"
      ]
    }
  },
  "scripts": {
    "pesy": "env PESY_MODE=update pesy",
    "fmt": "bash -c 'esy refmt --parse=re --in-place **/*.re'"
  },
  "dependencies": {
    "@esy-ocaml/reason": "3.5.2",
    "@opam/base": "v0.12.2",
    "@opam/cmdliner": "1.0.4",
    "@opam/core": "v0.12.4",
    "@opam/core_kernel": "v0.12.3",
    "@opam/ocurl": "0.9.0",
    "@opam/ppx_deriving": "4.4",
    "@opam/ppx_deriving_yojson": "3.5.1",
    "@opam/ppx_let": "v0.12.0",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "0.1.0",
    "@reason-native/pastel": "0.2.2",
    "@reason-native/rely": "3.1.0"
  },
  "devDependencies": {
    "@opam/dune": "1.11.4",
    "@opam/merlin": "3.3.2",
    "ocaml": "4.8.1000",
    "pesy": "0.4.4",
    "refmterr": "3.2.2"
  }
}

{
  "name": "dvm",
  "version": "1.1.0",
  "description": "Fast and simple Deno runtime version manager built with Native Reason",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "esy": {
    "build": "refmterr dune build --root . --profile release --ignore-promoted-rules --no-config",
    "buildsInSource": "_build"
  },
  "scripts": {
    "clean": "dune clean -p #{self.name}",
    "fmt": "esy dune build @fmt --auto-promote",
    "prettier": "bash -c 'prettier --write **/*.{json,md,yml}'"
  },
  "dependencies": {
    "@opam/cmdliner": "1.0.4",
    "@opam/core": "v0.13.0",
    "@opam/core_kernel": "v0.13.0",
    "@opam/dune": "2.0.1",
    "@opam/ezgzip": "0.2.3",
    "@opam/ocurl": "0.9.0",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "0.1.0",
    "@reason-native/pastel": "0.2.3",
    "ocaml": "4.08.1000",
    "refmterr": "3.3.0"
  },
  "devDependencies": {
    "@opam/merlin": "3.3.3",
    "prettier": "1.19.1"
  }
}

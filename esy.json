{
  "name": "dvm",
  "version": "1.1.0",
  "description": "Fast and simple Deno runtime version manager built with Native Reason",
  "repository": "https://github.com/imbsky/dvm",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "esy": {
    "build": "refmterr dune build --for-release-of-packages=#{self.name}",
    "buildsInSource": "_build"
  },
  "scripts": {
    "clean": "dune clean --for-release-of-packages=#{self.name}",
    "fmt": "esy dune build @fmt --auto-promote",
    "prettier": "bash -c 'prettier --write **/*.{json,md,yml}'"
  },
  "dependencies": {
    "@opam/cmdliner": "1.0.4",
    "@opam/core": "v0.13.0",
    "@opam/dune": "2.4.0",
    "@opam/ezgzip": "0.2.3",
    "@opam/ocurl": "0.9.0",
    "@opam/reason": "3.6.0",
    "@opam/yojson": "1.7.0",
    "@reason-native/console": "0.1.0",
    "@reason-native/pastel": "0.2.3",
    "ocaml": "4.9.0",
    "refmterr": "3.3.0"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "*",
    "prettier": "1.19.1"
  },
  "resolutions": {
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam#62f5c11"
  }
}
